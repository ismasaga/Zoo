<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox id="VBox" alignment="CENTER" prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zoo.LoginController">
  <padding>
    <Insets left="10.0" right="10.0" />
  </padding>
   <children>
      <Label fx:id="label1" text="Usuario:" />
      <TextField />
      <Label text="Clave:">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <PasswordField />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleButtonAction" text="Entrar">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
