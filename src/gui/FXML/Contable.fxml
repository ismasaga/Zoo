<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx/8.0.111" fx:controller="aplicacion.Controller.ContableController">
    <tabs>
        <Tab text="Avisos"/>
        <Tab text="Animais">
            <content>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <AnchorPane minHeight="62.0" prefHeight="62.0" prefWidth="800.0">
                            <children>
                                <TextField fx:id="buscarTextField" layoutX="228.0" layoutY="14.0"/>
                                <Button fx:id="buscarButton" layoutX="412.0" layoutY="14.0" mnemonicParsing="false"
                                        text="Buscar"/>
                                <Button fx:id="todosButton" layoutX="488.0" layoutY="14.0" mnemonicParsing="false"
                                        text="Ver Todos"/>
                            </children>
                        </AnchorPane>
                        <HBox prefHeight="510.0" prefWidth="800.0">
                            <children>
                                <Pane fx:id="panelAnimaisTabla" prefHeight="495.0" prefWidth="360.0">
                                    <HBox.margin>
                                        <Insets left="10.0"/>
                                    </HBox.margin>
                                </Pane>
                                <AnchorPane prefHeight="495.0" prefWidth="422.0">
                                    <children>
                                        <Label layoutX="26.0" layoutY="14.0" text="Nombre:"/>
                                        <Label layoutX="227.0" layoutY="69.0" text="Especie:"/>
                                        <Label layoutX="26.0" layoutY="69.0" text="Edad:"/>
                                        <Label layoutX="227.0" layoutY="128.0" text="Area:"/>
                                        <Label layoutX="24.0" layoutY="128.0" text="Xaula:"/>
                                        <TextField fx:id="textFieldNombre" layoutX="24.0" layoutY="30.0"/>
                                        <TextField fx:id="textFieldEspecie" layoutX="227.0" layoutY="85.0"/>
                                        <TextField fx:id="textFieldEdad" layoutX="24.0" layoutY="85.0"/>
                                        <TextField fx:id="textFieldArea" layoutX="227.0" layoutY="144.0"/>
                                        <TextField fx:id="textFieldXaula" layoutX="24.0" layoutY="144.0"/>
                                        <Button fx:id="buttonEliminar" layoutX="333.0" layoutY="455.0"
                                                mnemonicParsing="false" text="Eliminar"/>
                                        <Button fx:id="buttonGuardar" layoutX="240.0" layoutY="455.0"
                                                mnemonicParsing="false" text="Guardar"/>
                                        <Label layoutX="227.0" layoutY="14.0" text="Identificador:"/>
                                        <TextField fx:id="textFieldID" layoutX="227.0" layoutY="30.0"/>
                                        <Label layoutX="26.0" layoutY="188.0" text="Peso:"/>
                                        <Label layoutX="229.0" layoutY="188.0" text="Sexo:"/>
                                        <TextField fx:id="textFieldPeso" layoutX="26.0" layoutY="204.0"/>
                                        <ChoiceBox fx:id="choiceBoxSexo" layoutX="229.0" layoutY="204.0"
                                                   prefHeight="26.0" prefWidth="166.0"/>
                                    </children>
                                    <padding>
                                        <Insets left="15.0"/>
                                    </padding>
                                </AnchorPane>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0"/>
                            </padding>
                        </HBox>
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab text="Comida">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
        <Tab text="Personal">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
        <Tab text="Pechar Sesion">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Label layoutX="268.0" layoutY="253.0" text="Desexa pechar a sesion ou a aplicacion?"/>
                        <Button fx:id="sesionButton" layoutX="268.0" layoutY="288.0" mnemonicParsing="false"
                                text="Pechar sesion"/>
                        <Button fx:id="sairButton" layoutX="400.0" layoutY="288.0" mnemonicParsing="false"
                                text="Pechar aplicacion"/>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
